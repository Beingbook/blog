  "scripts": {
    "start": "run-p format:watch gatsby:dev",
    "gatsby:dev": "gatsby develop",
    "gatsby:build": "gatsby build",
    "gatsby:serve": "gatsby serve",
    "push": "gh-pages -d public -b master -r 'git@github.com:Beingbook/Beingbook.github.io.git'",
    "deploy": "run-s gatsby:build push",
    "format": "prettier --write ./**/*.{js,mdx,ts,tsx,css,scss}",
